                        "required": true
                    },
                    {
                        "description": "Изменение профиля пользователя",
                        "name": "updateProfileRequest",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/admin.updateProfileRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Профиль успешно изменен",
                        "schema": {
                            "type": "string"
